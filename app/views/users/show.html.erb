<div class="wrapper-white">
  <div class="row">
    <div class="col-xs-12 col-sm-4">
      <img src="http://via.placeholder.com/250x250" class="avatar">
    </div>
    <div class="col-xs-12 col-sm-4">
      <div class="user-data" style="flex: 1 1 auto;">
        <h3><%= @user.email %></h3>
      </div>
    </div>
    <div class="col-xs-12 col-sm-4">
      <div class="booking-form" style="text-align: center; width: 300px;">
        <h2>13€- first hour</h2>
        <h3><i class="glyphicon glyphicon-usd" style="color:#F86685;"></i>-7€ additional hours</h3>
        <form class="form-group">
          <div class="form-group">
            <input type="date" name="date" class="form-control">
          </div>
          <button class="btn btn-watermelon btn-lg">Schedule cleaning</button>
        </form>
      </div>
    </div>

  </div>
</div>
<!-- Services offered-->
<div class="wrapper-white">
  <div class="container">
    <h3>Services</h3>
    <div style="display: flex; justify-content: space-between;">
